(defun tocode (cos)
  "from cons of string to cons of integer"
  (mapcar (lambda (str)
	    (map 'cons
		 #'char-code
		 str))
	  cos))

(defun tohex (coi)
  "from cons of integer to hex"
  (let ((res nil))
    (labels ((f (coi res)
	       (if coi
		   (f (cdr coi)
		      (push (write-to-string (car coi) :base 16) res))
		   (reverse res))))
      (f coi res))))

